<?xml version="1.0" ?>
<Config>
    <RRTConnectSolver Name="rrt_connect">
        <Debug>0</Debug>
        <Smooth>1</Smooth>
        <ShortcutPath>1</ShortcutPath>
        <ReduceVertices>0</ReduceVertices>
        <Range>0.5</Range>
        <FinalInterpolationLength>50</FinalInterpolationLength>
    </RRTConnectSolver>

    <SamplingProblem Name="sampling_problem">
        <PlanningScene>
            <Scene>
                <JointGroup>whole_body</JointGroup>
                <URDF>{topological_memory_clustering}/examples/holonomic_se2_navigation/resources/Ada500.urdf</URDF>
                <SRDF>{topological_memory_clustering}/examples/holonomic_se2_navigation/resources/Ada500.srdf</SRDF>
                <LoadScene>{topological_memory_clustering}/examples/holonomic_se2_navigation/resources/cylinder_obstacle.scene</LoadScene>
                <SetRobotDescriptionRosParams>1</SetRobotDescriptionRosParams>
            </Scene>
        </PlanningScene>

        <Maps>
            <CollisionCheck Name="CC" SafeDistance="0.1"/>
        </Maps>

        <Equality>
            <Task Task="CC"/>
        </Equality>

        <StartState>-5 0 0</StartState>
        <Goal>5 0 0</Goal>
        
        <FloatingBaseLowerLimits>-10 -10 0</FloatingBaseLowerLimits>
        <FloatingBaseUpperLimits> 10  10 3.1415</FloatingBaseUpperLimits>
    </SamplingProblem>
</Config>
